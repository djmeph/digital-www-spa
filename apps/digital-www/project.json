{
  "name": "digital-www",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/digital-www",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/digital-www"
      },
      "configurations": {
        "production": {},
        "development": {
          "outputPath": "apps/digital-www"
        }
      }
    },
    "docker-build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "docker build -t digital-www ./dist/apps/digital-www"
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "options": {
        "buildTarget": "digital-www:build",
        "dev": true
      },
      "configurations": {
        "production": {
          "buildTarget": "digital-www:build:production",
          "dev": false
        },
        "development": {
          "buildTarget": "digital-www:build:development",
          "dev": true,
          "port": 42000,
          "hostname": "0.0.0.0"
        }
      },
      "defaultConfiguration": "development"
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "digital-www:build:production"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/apps/digital-www"
      ],
      "options": {
        "jestConfig": "apps/digital-www/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/digital-www/**/*.{ts,tsx,js,jsx}"
        ]
      }
    }
  },
  "tags": []
}
